<div class="box box-primary">
  <div class="box-header with-border">
    <h3 class="box-title">Nova Consulta</h3>
  </div>
<%= form_with(model: answear, local: true) do |form| %>
  <% if answear.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(answear.errors.count, "error") %> prohibited this answear from being saved:</h2>

      <ul>
      <% answear.errors.full_messages.each do |message| %>
